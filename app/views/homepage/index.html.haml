:javascript
  $(function () {
    $('.carousel').carousel({
      interval: 5000, 
    });
  });
:css
  .title {
    text-align: center;
  }
  .item {
    overflow:hidden;
  }
  .item img {
    margin: -30px 0px -200px 0px;
    position: relative;
    z-index: 0;
    opacity: 0.4;
  }
  .fb-like {
    background: white;
  }
.container
  .row
    = # %h1.title Lonesome Giant
    #mainCarousel.carousel.slide
      .carousel-inner
        .item 
          %img(src="/band_main.jpg")
        .item
          %img(src="/band_main.jpg")
        .item
          %img(src="/band_main.jpg")
  .row
    .span4
      = render "shows"
    .span4
      = render "music"
    .span4
      <div style="position: absolute; top: 100px;"class="fb-like" data-href="http://www.facebook.com/lonesomegiant" data-send="true" data-width="300" data-show-faces="true"></div>
